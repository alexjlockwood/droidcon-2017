{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "target": "es5",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "typeRoots": ["node_modules/@types"],
    "outDir": "./dist/out-tsc",
    "lib": ["es2017", "dom"],
    "allowJs": true
  },
  "exclude": ["node_modules", "dist"]
}
