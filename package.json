    "@types/d3-queue": "^3.0.5",
    "@types/d3-request": "^1.0.0",
    "@types/d3-scale": "^1.0.0",
    "@types/d3-selection": "^1.0.0",
    "@types/d3-selection-multi": "^1.0.0",
    "@types/d3-timer": "^1.0.0",
    "@types/d3-transition": "^1.0.0",
    "@types/lodash-es": "^4.14.7",
    "gh-pages": "^1.0.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.1",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
