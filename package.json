    "d3-jetpack": "^2.0.13",
    "d3-polygon": "^1.0.3",
    "d3-queue": "^3.0.7",
    "d3-request": "^1.0.6",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-selection-multi": "^1.0.1",
    "d3-timer": "^1.0.7",
    "d3-transition": "^1.1.0",
    "earcut": "^2.1.1",
    "lodash-es": "^4.17.4",
    "svgpath": "^2.2.1",
    "topojson-client": "^3.0.0"
  },
