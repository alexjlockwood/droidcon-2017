{
  "name": "shape-shifter-demos",
  "version": "1.0.0",
  "description": "Shape Shifter demos.",
  "scripts": {
    "prestart": "tsc && rollup -c",
    "start": "npm-run-all --parallel serve tsc:w rollup:w",
    "serve": "live-server src src/demos --watch=src/**/*.html,src/bundle.js,src/**/*.css",
    "tsc:w": "tsc -w",
    "rollup:w": "rollup -c -w"
  },
  "keywords": [],
  "author": "Alex Lockwood",
  "license": "ISC",
  "dependencies": {
    "d3": "^4.11.0",
    "d3-jetpack": "^2.0.13",
    "d3-selection-multi": "^1.0.1",
    "lodash-es": "^4.17.4"
  },
  "devDependencies": {
    "@types/d3": "^4.11.0",
    "@types/d3-selection-multi": "^1.0.6",
    "@types/lodash-es": "^4.14.7",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.1",
    "rollup": "^0.50.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-watch": "^4.3.1",
    "typescript": "^2.5.3"
  }
}
